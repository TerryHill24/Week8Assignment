<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#C3E8C5"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/txtHeading"
        android:layout_width="343dp"
        android:layout_height="41dp"
        android:layout_marginStart="37dp"
        android:layout_marginTop="124dp"
        android:layout_marginEnd="31dp"
        android:layout_marginBottom="566dp"
        android:text="Enter Shipping Information Below"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/txtName"
        android:layout_width="346dp"
        android:layout_height="57dp"
        android:layout_marginStart="34dp"
        android:layout_marginTop="172dp"
        android:layout_marginEnd="31dp"
        android:layout_marginBottom="502dp"
        android:ems="10"
        android:hint="Full Name"
        android:inputType="textPersonName"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/txtAddress"
        android:layout_width="342dp"
        android:layout_height="54dp"
        android:layout_marginStart="34dp"
        android:layout_marginTop="242dp"
        android:layout_marginEnd="35dp"
        android:layout_marginBottom="435dp"
        android:ems="10"
        android:hint="Address"
        android:inputType="textPostalAddress"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/txtCity"
        android:layout_width="346dp"
        android:layout_height="57dp"
        android:layout_marginStart="34dp"
        android:layout_marginTop="311dp"
        android:layout_marginEnd="31dp"
        android:layout_marginBottom="363dp"
        android:ems="10"
        android:hint="City"
        android:inputType="textPersonName"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/txtState"
        android:layout_width="165dp"
        android:layout_height="58dp"
        android:layout_marginStart="31dp"
        android:layout_marginTop="387dp"
        android:layout_marginEnd="215dp"
        android:layout_marginBottom="286dp"
        android:ems="10"
        android:hint="State"
        android:inputType="textPersonName"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/txtZip"
        android:layout_width="155dp"
        android:layout_height="57dp"
        android:layout_marginStart="221dp"
        android:layout_marginTop="387dp"
        android:layout_marginEnd="35dp"
        android:layout_marginBottom="287dp"
        android:ems="10"
        android:hint="ZIP Code"
        android:inputType="number"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/txtEMail"
        android:layout_width="345dp"
        android:layout_height="56dp"
        android:layout_marginStart="31dp"
        android:layout_marginTop="465dp"
        android:layout_marginEnd="35dp"
        android:layout_marginBottom="210dp"
        android:ems="10"
        android:hint="E-Mail"
        android:inputType="textEmailAddress"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnPage2"
        android:layout_width="343dp"
        android:layout_height="65dp"
        android:layout_marginStart="31dp"
        android:layout_marginTop="530dp"
        android:layout_marginEnd="37dp"
        android:layout_marginBottom="136dp"
        android:background="#B19F9E"
        android:text="SUBMIT"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>